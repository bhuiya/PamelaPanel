<%= form_for @course do |f| %>

     <div>
                 <%= f.label :course_name %>
                 <%= f.text_field :course_name %>
     </div>
     <div>
                 <%= f.label :total_hours %>
                 <%= f.text_field :total_hours %>
     </div>
     <div>
                   <%= f.submit %>
     </div>

<% end %>
