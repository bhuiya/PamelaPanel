<%= render 'form.index' %>
<div class="backend_header" id="main-header" class="py-2 bg-primary text-white">
 <div class="container">
  <div class="row">
    <div class="col-md-6">
     <h1><i class="fa fa-graduation-cap" aria-hidden="true"></i>  Student</h1>
    </div>
   </div>
  </div>
 </div>

<center>
 <h2> Instructor Detail Information</h2>
 <div class="card" style="width: 30rem;">
  <div class="card-body">
    <h4 class="card-title">First Name</h4>
    <p class="card-text">  <%= @instructor.first_name %></p>
   </div>
   <div class="card-body">
    <h4 class="card-title">Last Name</h4>
    <p class="card-text">  <%= @instructor.last_name %></p>
   </div>
   <div class="card-body">
    <h4 class="card-title">Date Of Birth</h4>
    <p class="card-text">  <%= @instructor.dob %></p>
   </div>
   <div>
    <h4 class="card-title">Highest Education</h4>
    <p class="card-text">  <%= @instructor.highest_education %></p>
   </div>
   <div>
    <h4 class="card-title">Salary</h4>
    <p class="card-text">  <%= @instructor.salary %></p>
   </div>
   <div class="card-body">
       <%= link_to "Back to Instructors", instructors_path %>
   </div>
  </div>
</center>
